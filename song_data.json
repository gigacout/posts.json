        { "id": "line3", "base_duration": 3.0, "texts": {"en": "Would it be the same", "es": "¿Sería lo mismo?", "fr": "Serait-ce pareil", "de": "Wäre es dasselbe", "pt": "Seria o mesmo", "it": "Sarebbe lo stesso", "zh-TW": "一切會一樣嗎", "zh-CN": "一切会一样吗"} },
        { "id": "line4", "base_duration": 4.0, "texts": {"en": "If I saw you in heaven?", "es": "¿Si te viera en el cielo?", "fr": "Si je te voyais au paradis?", "de": "Wenn ich dich im Himmel sähe?", "pt": "Se eu te visse no céu?", "it": "Se ti vedessi in paradiso?", "zh-TW": "如果我在天堂遇見你？", "zh-CN": "如果我在天堂遇见你？"} },
        { "id": "line5", "base_duration": 2.0, "texts": {"en": "I must be strong", "es": "Debo ser fuerte", "fr": "Je dois être fort", "de": "Ich muss stark sein", "pt": "Eu devo ser forte", "it": "Devo essere forte", "zh-TW": "我必須堅強", "zh-CN": "我必须坚强"} },
        { "id": "line6", "base_duration": 2.0, "texts": {"en": "And carry on", "es": "Y seguir adelante", "fr": "Et continuer", "de": "Und weitermachen", "pt": "E seguir em frente", "it": "E andare avanti", "zh-TW": "並繼續前行", "zh-CN": "并继续前行"} },
