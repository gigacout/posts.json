        { "id": "line0", "base_duration": 14.0, "isMelody": true, "texts": {"en": "♫♬♪♩𝄞 Intro", "es": "♫♬♪♩𝄞 Intro", "fr": "♫♬♪♩𝄞 Intro", "de": "♫♬♪♩𝄞 Intro", "pt": "♫♬♪♩𝄞 Intro", "it": "♫♬♪♩𝄞 Intro", "zh-TW": "♫♬♪♩𝄞 前奏", "zh-CN": "♫♬♪♩𝄞 前奏"} },
        { "id": "line1", "base_duration": 3.0, "texts": {"en": "Would you know my name", "es": "¿Sabrías mi nombre?", "fr": "Connaîtrais-tu mon nom", "de": "Würdest du meinen Namen kennen", "pt": "Você saberia meu nome", "it": "Sapresti il mio nome", "zh-TW": "你會知道我的名字嗎", "zh-CN": "你会知道我的名字吗"} },
