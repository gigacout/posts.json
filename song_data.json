        { "id": "line10", "base_duration": 3.0, "texts": {"en": "Would you hold my hand", "es": "¿Sostendrías mi mano?", "fr": "Tiendrais-tu ma main", "de": "Würdest du meine Hand halten", "pt": "Você seguraria minha mão", "it": "Mi terresti la mano", "zh-TW": "你會牽我的手嗎", "zh-CN": "你会牵我的手吗"} },
        { "id": "line11", "base_duration": 4.0, "texts": {"en": "If I saw you in heaven?", "es": "¿Si te viera en el cielo?", "fr": "Si je te voyais au paradis?", "de": "Wenn ich dich im Himmel sähe?", "pt": "Se eu te visse no céu?", "it": "Se ti vedessi in paradiso?", "zh-TW": "如果我在天堂遇見你？", "zh-CN": "如果我在天堂遇见你？"} },
        { "id": "line12", "base_duration": 3.0, "texts": {"en": "Would you help me stand", "es": "¿Me ayudarías a levantarme?", "fr": "M'aiderais-tu à me relever", "de": "Würdest du mir helfen aufzustehen", "pt": "Você me ajudaria a levantar", "it": "Mi aiuteresti ad alzarmi", "zh-TW": "你會扶我站起來嗎", "zh-CN": "你会扶我站起来吗"} },
        { "id": "line13", "base_duration": 4.0, "texts": {"en": "If I saw you in heaven?", "es": "¿Si te viera en el cielo?", "fr": "Si je te voyais au paradis?", "de": "Wenn ich dich im Himmel sähe?", "pt": "Se eu te visse no céu?", "it": "Se ti vedessi in paradiso?", "zh-TW": "如果我在天堂遇見你？", "zh-CN": "如果我在天堂遇见你？"} },
